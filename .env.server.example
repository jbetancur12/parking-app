# Server Environment Variables
# Copy this to your VPS as .env.server

# Database
DB_USER=postgres
DB_PASSWORD=your_db_password_here
DB_NAME=parking_db

# Authentication
JWT_SECRET=your_jwt_secret_here

# Email (Resend)
RESEND_API_KEY=your_resend_api_key_here
RESEND_FROM_EMAIL=Aparca <noreply@your-domain.com>

# Frontend URL for CORS and emails
FRONTEND_URL=https://parking.betancur.work

# Environment
NODE_ENV=production
