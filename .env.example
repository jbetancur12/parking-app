# Database Configuration
# Used by Postgres container and Server
DB_USER=postgres
DB_PASSWORD=secure_password_here
DB_NAME=parking_db

# Security
# Used by Server for signing tokens
JWT_SECRET=replace_with_a_secure_long_random_string

# Email Service (Resend)
# Used by Server to send emails
RESEND_API_KEY=re_123456789
RESEND_FROM_EMAIL=onboarding@resend.dev

# URLs
# Used by Server for CORS and Client for API connection
FRONTEND_URL=https://your-domain.com
VITE_API_URL=https://api.your-domain.com/api
